library(tidyverse)
library(lubridate)
library(httr)
library(jsonlite)
library(msthemes)
library(geojsonsf)
library(sf)
library(scales)
stations <- tribble(
~id, ~name,
105, "Wien",
131, "Salzburg",
93, "Niederösterreich",
56, "Oberösterreich",
48, "Kärnten",
39, "Tirol",
30, "Steiermark",
22, "Burgenland",
15, "Vorarlberg"
)
res.api = GET(glue::glue("https://dataset.api.hub.zamg.ac.at/v1/station/historical/klima-v1-1d?parameters=tmax&start=1950-01-01&end=2022-12-31&station_ids=",paste0(stations$id,collapse = ",")))
data <- fromJSON(rawToChar(res.api$content))
tempdat <- tribble(~time, ~id, ~tmax)
for (i in 1:nrow(stations)) {
add.dat <- data.frame(time = as.Date(data[["timestamps"]]),
id = as.numeric(data[["features"]][["properties"]][["station"]][[i]]),
tmax = data[["features"]][["properties"]][["parameters"]][["tmax"]][["data"]][[i]])
tempdat <- bind_rows(tempdat, add.dat)
}
blmap <- geojson_sf("https://raw.githubusercontent.com/ginseng666/GeoJSON-TopoJSON-Austria/master/2021/simplified-95/laender_95_geo.json")
findat <- tempdat |> left_join(stations) |>
mutate(time = ymd(time),
year = year(time),
decade = glue::glue("{year(floor_date(time, '10 years'))}er")) |>
filter(tmax >= 30)
# Frühester Tag mit 30+ Grad
findat |> slice_min(time, by = year, with_ties = F) |>
ggplot(aes(x = year, y = lubridate::`year<-`(time, 2000))) +
geom_line() + scale_y_date(labels = date_format("%e. %b", locale = "de")) +
geom_smooth(method=lm, se = F, color = "darkred") +
labs(x = NULL, y = NULL) +
theme_minimal()
yeardat <- findat |> count(year, name, .drop = FALSE) |>
complete(year, name, fill = list(n = 0))
decadedat <- findat |> count(year, name, decade, .drop = FALSE) |>
summarise(n = mean(n), .by = c(name, decade))
decadedat
yeardat |> arrange(n)
yeardat |> arrange(desc(n))
